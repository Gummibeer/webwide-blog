{
  "name": "webwide",
  "version": "1.0.0",
  "description": "Custom Webwide Blog theme for Grav CMS",
  "watch": {
    "compile": {
      "patterns": [
        "scss"
      ],
      "extensions": "js,scss",
      "quiet": false
    }
  },
  "scripts": {
    "watch": "npm-watch compile",
    "compile": "node-sass scss/style.scss -o css"
  },
  "author": "Adam Greenough",
  "license": "All Rights Reserved",
  "dependencies": {
    "node-sass": "^4.13.0",
    "npm-watch": "^0.6.0",
    "uglify-es": "^3.3.9"
  }
}
