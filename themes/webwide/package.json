{
  "name": "webwide-blog",
  "version": "1.0.0",
  "description": "",
  "watch": {
    "compile": {
      "patterns": [
        "src"
      ],
      "extensions": "js,scss",
      "quiet": false
    }
  },
  "scripts": {
    "watch": "npm-watch compile",
    "compile": "uglifyjs src/js/scripts.js --mangle --output dist/scripts.js & node-sass src/scss/style.scss -o dist"
  },
  "author": "Adam Greenough",
  "license": "All Rights Reserved",
  "dependencies": {
    "node-sass": "^4.13.0",
    "npm-watch": "^0.6.0",
    "uglify-es": "^3.3.9"
  }
}
